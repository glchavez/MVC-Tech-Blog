<!-- Loop over each object in the post array -->
<h1>The Tech Blog</h1>

{{#each posts as |post| }}
<div class="card">
  <h4 class="card-header">{{post.title}} Posted by {{user.username}} on {{format_date post.date_created}}</h4>
  <div class="card-body">
    <p class="card-text">{{post.content}}</p>
  </div>
</div>
{{/each}}